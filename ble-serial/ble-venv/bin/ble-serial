#!/home/riveria/elm327/raspberry/ble-serial/ble-venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ble-serial==2.6.1','console_scripts','ble-serial'
__requires__ = 'ble-serial==2.6.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ble-serial==2.6.1', 'console_scripts', 'ble-serial')()
    )
